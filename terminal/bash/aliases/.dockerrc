#! /bin/bash

# Docker Shortcuts
#---------------#
function dockertags () {
  $(command -v curl) -sS "https://registry.hub.docker.com/v1/repositories/$1/tags" | jq '.[]["name"]' | cut -d'"'| sort -n
}

function docker-inspect-images() {
  $(command -v docker) inspect "$@" --format "{{.RepoTags}} {{.Architecture}}"
}
